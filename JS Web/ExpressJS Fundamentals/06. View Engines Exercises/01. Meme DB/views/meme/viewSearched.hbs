<h1>Result from your search</h1>

<ul class="meme_list">
    <div class="container">
    {{#each memes}}
        <div class="meme">
            <a href="/getDetails?id={{this._id}}">
                <img class="memePoster" src="{{this.memePath}}">
            </a>
        </div>
    {{/each}}
    </div>
</ul>