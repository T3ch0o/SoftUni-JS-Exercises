<h1>Welcome to SoftUni Wiki!</h1>
<section>
    <h2>Search results for: "{{searchParam}}"</h2>
    <div class="spacer">
        <ul>
            {{#each articles}}
                <li>
                    <a href="/article/{{this._id}}/{{this.editId}}">{{this.title}}</a>
                </li>
                {{else}}
                <li>This search is bullshit</li>
            {{/each}}
        </ul>
    </div>
</section>